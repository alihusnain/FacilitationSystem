@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>About</h2>

@using EADWebTeacherFacilitationSystem.Models
<body>
    <div class="container">

        <table class="table table-striped table-bordered table-hover">
            <tr><th>Mission</th><th>Vision</th><th>About Us</th><th>Update</th></tr>

            @foreach (About i in Model)
            {
                <tr><td>@i.Mission</td><td>@i.Vision</td><td>@i.AboutUs</td><td> <a href="@("../EditWebsite/EditAbout/" + i.Id)">   <input class="btn btn-primary" type="button" value="Edit"> </a> </td></tr>
            }

        </table>

    </div>


</body>
